Public Sub countryCodeView()
    response.write "function inNationalCode(nat) {" & vbcrLf &_
                   "    var countryCode = new Array();"
    for i=0 to uBound(countryCodeListArr)
        response.write "countryCode["&i&"] = '" & countryCodeListArr(i) & "';"
    next
    
    response.write "var form = document.signForm;" &_
                   "if (countryCode[nat-1] != null) {" &_
                   "form.phone_01.value = countryCode[nat-1];" &_
									 "form.mobile_01.value = countryCode[nat-1];" &_
                   "} else {" &_
                   "form.phone_01.value = '';" &_
                   "form.mobile_01.value = '';" &_
                   "}" & vbcrLf &_
                   "}"
                   
End Sub


Public Sub countryCodeView_1()
